{
  "name": "ecommerce-express",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/RicardoGA18/ecommerce-express.git",
  "author": "Ricardo Gambini <ricardo.gambini18@outlook.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "build": "babel src --out-dir build",
    "test": "cross-env NODE_ENV=test jest --verbose --forceExit --maxWorkers=1",
    "test:silent": "yarn test -- --silent",
    "test:watch": "yarn test -- --watch"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.7",
    "multer": "^1.4.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "babel-jest": "^26.6.3",
    "dotenv": "^9.0.0",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
